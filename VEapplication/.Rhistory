library("tidyverse")
x1 <- c(1,2,3,4)
y1 <- c(2,4,6,8)
z1 <- c(3,5,7,9)
df1 <- data.frame(x1, y1, z1)
x2 <- c(2,4,1,3)
y2 <- c(10,11,12,13)
z2 <- c(25,24,23,22)
df2 <- data.frame(x2, y2, z2)
inner_join(df1, df2, by=(x1==x2))
library("tidyverse")
x1 <- c(1,2,3,4)
y1 <- c(2,4,6,8)
z1 <- c(3,5,7,9)
df1 <- data.frame(x1, y1, z1)
x2 <- c(2,4,1,3)
y2 <- c(10,11,12,13)
z2 <- c(25,24,23,22)
df2 <- data.frame(x2, y2, z2)
inner_join(df1, df2, by=join_by(x1==x2))
library("tidyverse")
x  <- c(1,2,3,4)
y1 <- c(2,4,6,8)
z1 <- c(3,5,7,9)
df1 <- data.frame(x, y1, z1)
x  <- c(2,4,1,3)
y2 <- c(10,11,12,13)
z2 <- c(25,24,23,22)
df2 <- data.frame(x, y2, z2)
inner_join(df1, df2, by=join_by(x))
inner_join(df2, df1, by=join_by(x))
